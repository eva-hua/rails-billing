               </tr>
               <tr>
                  <td>{{ 'summary.month' | translate }}</td>
                  <td>{{ bill_summary.income.month }}</td>
                  <td>{{ bill_summary.expense.month }}</td>
               </tr>
               <tr>
                  <td>{{ 'summary.total' | translate }}</td>
